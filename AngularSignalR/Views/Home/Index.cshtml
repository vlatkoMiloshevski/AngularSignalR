@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" ng-app="dogApp">
<head>
</head>
<body>
    <a ui-sref="home.walk">walk controller</a>
    <a ui-sref="home.employee">employee controller</a>

    <div ui-view id="mainContent" ng-controller="MainCtrl">
    </div>
    <!-- Third party scripts -->
    <script src="~/Scripts/jquery-2.2.3.min.js"></script>
    <script src="~/Scripts/jquery.signalR-2.2.0.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-ui.js"></script>
    <script src="~/Scripts/angular-ui-router.js"></script>
    <script src="~/Scripts/angular-animate.js"></script>
    <script src="~/Scripts/angular-sanitize.js"></script>
    <script src="signalr/hubs"></script>
    <script>
        var app = angular.module('dogApp', ['ui.router', 'ngSanitize', 'Home', 'Walk', 'Employee']);
        app.constant('appBaseUrl', '@ViewBag.AppBaseUrl');
        app.constant('apiUrl', '@ViewBag.ApiUrl');
    </script>
    <!-- angular module scripts -->
    <script src="~/app/app.js" type="text/javascript"></script>
    <script src="~/app/Home/Home.js" type="text/javascript"></script>
    <script src="~/app/Walk/Walk.js" type="text/javascript"></script>
    <script src="~/app/Employee/Employee.js" type="text/javascript"></script>
</body>
</html>
